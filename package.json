{
  "name": "simple-ecommerce",
  "version": "1.0.0",
  "description": "TypeScript Express API with MySQL and Sequelize for E-commerce",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:watch": "nodemon --watch 'src/**/*.ts' --exec ts-node src/server.ts",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:status": "npx sequelize-cli db:migrate:status",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:create": "npx sequelize-cli db:create",
    "db:setup": "node setup-db.js",
    "lint": "echo \"Linting not configured yet\"",
    "test": "npx jest",
    "test:category": "npx jest tests/category.test.ts",
    "test:product": "npx jest tests/product.test.ts",
    "test:product-simple": "jest tests/product-simple.test.ts",
    "test:product-working": "jest tests/product-working.test.ts",
    "test:debug": "jest tests/debug-product.test.ts",
    "test:manual": "jest tests/manual-debug.test.ts",
    "test:product-final": "jest tests/product-final.test.ts",
    "test:product-complete": "jest tests/product-complete.test.ts",
    "test:all": "jest tests/"
  },
  "keywords": [
    "typescript",
    "express",
    "mysql",
    "sequelize",
    "rest-api"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "mysql2": "^3.14.3",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "^13.15.2",
    "class-validator": "^0.14.2",
    "jest": "^30.1.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
